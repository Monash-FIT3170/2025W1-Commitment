{
  "name": "gitgauge",
  "version": "0.2.0",
  "description": "pretty gui for git contribution stats",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tauri": "tauri",
    "fmt": "npx prettier --check .",
    "fmt:fix": "npx prettier --write .",
    "rs-fmt": "cargo fmt --check --manifest-path src-tauri/Cargo.toml",
    "rs-fmt:fix": "cargo fmt --manifest-path src-tauri/Cargo.toml",
    "clippy": "cargo clippy --manifest-path src-tauri/Cargo.toml",
    "clippy:fix": "cargo clippy --manifest-path src-tauri/Cargo.toml --fix"
  },
  "license": "MIT",
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-dialog": "^2.3.3",
    "@tauri-apps/plugin-fs": "^2.4.2",
    "@tauri-apps/plugin-log": "^2.4.0",
    "@tauri-apps/plugin-opener": "^2",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "echarts": "^5.6.0",
    "flatpickr": "^4.6.13"
  },
  "devDependencies": {
    "@iconify/svelte": "^5.0.1",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.20.8",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tauri-apps/cli": "^2",
    "svelte": "^5.28.2",
    "svelte-check": "^4.1.7",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte-echarts": "^1.0.0",
    "typescript": "~5.6.2",
    "vite": "^6.3.5"
  },
  "overrides": {
    "devalue": ">=5.3.2",
    "cookie": "^0.7.0"
  }
}
