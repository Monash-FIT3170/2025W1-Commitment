{
  "name": "gitgauge",
  "version": "0.2.0",
  "description": "pretty gui for git contribution stats",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "tauri": "tauri",
    "fmt": "npx prettier --write .",
    "fmt:check": "npx prettier --check .",
    "rs-fmt": "cargo fmt --manifest-path src-tauri/Cargo.toml",
    "rs-fmt:check": "cargo fmt --check --manifest-path src-tauri/Cargo.toml"
  },
  "license": "MIT",
  "dependencies": {
    "@tauri-apps/api": "^2",
    "@tauri-apps/plugin-log": "^2.4.0",
    "@tauri-apps/plugin-opener": "^2",
    "echarts": "^5.6.0",
    "flatpickr": "^4.6.13",
    "svelte-echarts": "^1.0.0"
  },
  "devDependencies": {
    "@iconify/svelte": "^5.0.0",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.9.0",
    "@sveltejs/vite-plugin-svelte": "^5.0.0",
    "@tauri-apps/cli": "^2",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "svelte-echarts": "^1.0.0",
    "typescript": "~5.6.2",
    "vite": "^6.3.5"
  }
}
